{
    "name": "asmotor-syntax",
    "displayName": "ASMotor",
    "description": "Syntax highlighting for the ASMotor assembly language",
    "version": "1.0.0",
    "engines": {
        "vscode": "^1.36.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "asm.680x0",
            "aliases": ["ASMotor.680x0", "asm"],
            "extensions": [".asm",".68k",".inc"],
            "configuration": "./language-configuration.json"
        },{
            "id": "asm.z80",
            "aliases": ["ASMotor.Z80", "asm"],
            "extensions": [".asm",".z80",".inc"],
            "configuration": "./language-configuration.json"
        },{
            "id": "asm.6502",
            "aliases": ["ASMotor.6502", "asm"],
            "extensions": [".asm",".6502",".inc"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "asm.680x0",
            "scopeName": "source.asm.680x0",
            "path": "./syntaxes/asm.680x0.tmLanguage.json"
        },{
            "language": "asm.z80",
            "scopeName": "source.asm.z80",
            "path": "./syntaxes/asm.z80.tmLanguage.json"
        },{
            "language": "asm.6502",
            "scopeName": "source.asm.6502",
            "path": "./syntaxes/asm.6502.tmLanguage.json"
        },{
            "injectTo": ["source.asm.680x0", "source.asm.z80", "source.asm.6502"],
            "scopeName": "source.asm",
            "path": "./syntaxes/asm.tmLanguage.json"
        }]
    }
}